.container {
  height: 100vh;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.centered{
  display:flex;
  flex-direction:column;
  align-items:center;
}
.vertical.centered > view{
  display:flex;
  flex-direction:row;
  height:100vh;
  align-items:center;
}
